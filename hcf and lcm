#HCF AND LCM
import math
max=0
min=0
x=int(input("input the first number:"))
y=int(input("input the second number"))
if x>y:
    min=y
    max=x
elif y>x:
    max=y
    min=x
elif x==y:
    min=x
    max=y
if max%min==0:
    print(min,"is the hcf")
    hcf=min
else:
    for i in range(1,int(min/2)):
        if min%i==0 and max%i==0:
            hcf=i
    print(min,"is the hcf")
lcm=int(x*y/hcf)
print(lcm,"is the lcm")
    
    
